{
    "version": 2,
    "builds": [
      {
        "src": "/api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/anomalies(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/test",
        "dest": "/api/index.js"
      },
      {
        "src": "/",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "crons": [
      {
        "path": "/api/cronAnomaly.js",
        "schedule": "*/30 * * * *"
      }
    ]
  }
