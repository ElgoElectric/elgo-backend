{
    "version": 2,
    "builds": [
      {
        "src": "/api/index.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/anomalies/:path*",
        "destination": "/api/index.js"
      },
      {
        "source": "/test",
        "destination": "/api/index.js"
      },
      {
        "source": "/",
        "destination": "/api/index.js"
      },
      {
        "source": "/:path*",
        "destination": "/api/index.js"
      }
    ],
    "crons": [
      {
        "path": "/api/cronAnomaly",
        "schedule": "*/30 * * * *"
      }
    ]
  }
